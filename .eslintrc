{
  "parser": "babel-eslint",
  "extends": [
    "react-app",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 0,
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "no-underscore-dangle": 0,
    "react/state-in-constructor": 0,
    "no-plusplus": 0,
    "no-console": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ]
  }
}